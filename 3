#include <iostream>
using namespace std;
int main()
{
    int N, A, B;
    int a[20];
    cout << "Enter N, A, B: \n";
    cin >> N >> A >> B;
    a[0] = A;
    a[1] = B;
    for (int i = 2; i < N; i++) {
        a[i] = a[i - 2] + a[i - 1];
    }
    for (int i = 0; i < N; i++) {
        cout << a[i] << " ";
    }
}
